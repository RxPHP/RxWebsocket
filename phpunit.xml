        processIsolation="false"
        stopOnFailure="false"
        syntaxCheck="false"
        bootstrap="test/bootstrap.php">
    <testsuites>
        <testsuite name="RxWebsocket Test">
            <directory>test/</directory>
        </testsuite>
    </testsuites>

</phpunit>